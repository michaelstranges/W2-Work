<!-- views/partials/_header.ejs -->

<meta charset="UTF-8">

<h1>TinyApp</h1>

<% if(user === undefined){  %>

  <a href="/register">Register</a>
  <a href="/login">Login</a>

<% } else { %>

  <form method="POST" action="/logout">
    <label><b>USER:</b> <%= user.email %></label>
    <input type="submit" value="logout">
  </form>

<% } %>



