<!DOCTYPE html>

<html lang="en">
  <head>
    <title>My TinyApp</title>
  </head>

  <body>
    <header>
      <% include ./partials/_header.ejs %>
    </header>
    <br>

    <% for (userID in urls){ %>
      <% if(userID === user.id){ %>
        <% for(links in urls[userID]){ %>
              <ul>
                <form method="POST" action= <%= `/urls/${links}/delete` %>>
                <li>
                  <%= "id: " + links + " " + urls[userID][links] %>
                  <a href=<%= `/urls/${links}` %>>Edit</a>
                  <button type="submit">Delete</button>
               </li>
               </form>
              </ul>
         <% } %>
      <%} %>
  <% } %>
    <br>
      <a href="/urls/new">Shorten a Link!</a>
    <br>

  </body>

</html>